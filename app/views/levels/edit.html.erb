<div class="card border-info">
  <div class="card-header" id='card-header'>
    <div class="row">
      <div class="col-md-10">
        <h1><b class="text">Editar Nivel</b></h1>
      </div>
      <div class="col-md-2">
        <%= link_to 'Regresar', levels_path, class: "text-white btn btn-warning float-right" %>
        <%= link_to 'Mostrar', @level, class: "text-white btn btn-info float-right" %>

      </div>
    </div>
  </div>
  <div class="card-body">
    <%= render 'form', level: @level %>
  </div>
</div>
